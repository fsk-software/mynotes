<resources>
	<declare-styleable name="noteAttrs">
		<attr name="colorType">
  			<enum name="yellow" value="0" />
  			<enum name="blue" value="1" />
  			<enum name="green" value="2" />
  			<enum name="pink" value="3" />
  			<enum name="grey" value="4" />
  		</attr>
		<attr name="allowMultiSelect" format="boolean"/>
		<attr name="onWidth" format="dimension"/>
		<attr name="onHeight" format="dimension"/>
		<attr name="offWidth" format="dimension"/>
		<attr name="offHeight" format="dimension"/>
		<attr name="minChildDimX" format="dimension"/>
		<attr name="minChildDimY" format="dimension"/>
		<attr name="spacing" format="dimension"/>
		<attr name="orientation">
  			<enum name="horizontal" value="0" />
  			<enum name="vertical" value="1" />
  		</attr>
	</declare-styleable>
</resources>